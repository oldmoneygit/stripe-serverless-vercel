{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/create-checkout.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/stripe.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/ping.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "pages/api/create-checkout",
      "dest": "/api/create-checkout.js"
    },
    {
      "src": "pages/api/stripe",
      "dest": "pages/api/stripe.js"
    },
    {
      "src": "pages/api/ping",
      "dest": "pages/api/ping.js"
    }
  ]
}
